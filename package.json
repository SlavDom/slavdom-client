    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "scripts": {
    "start_new": "babel src --out-dir build && ./build/startServer.js | ./node_modules/.bin/bunyan",
    "start": "webpack-dev-server",
    "fix": "eslint ./src/**/js* --fix",
    "compile": "tsc"
